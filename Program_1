public class Test {
	public static void main(String args[]) {
		String firstName = "edu";
		String lastName = new String("bridge");
		System.out.println(firstName.length()+lastName.length());
		System.out.println(firstName.compareTo(lastName) < 0  ? "Yes" : "No"); 
		System.out.println(firstName.substring(0).toUpperCase()+" ".concat(lastName.substring(0).toUpperCase()));
	}
}
